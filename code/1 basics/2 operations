machines = ['iMac', 'MacPro', 'RackMac']
apps = ['Keynote', 'Office', 'AdobeReader', 'MacKeeper']

# more examples of a function
len(machines)
how_many_types = len(machines)

# len() returns a value, an integer
len(machines) + len(apps)
